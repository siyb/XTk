<xtk namespace="foo" ttk="1">
	<pack>
		<frame variable="xyz">
			<pack fill="both" expand="1" side="right">
				<button text="Hi there" variable="hitherebutton" command="exit" />
			</pack>
			<pack fill="both" expand="0" side="left" before="@hitherebutton">
				<label text="TestLable" variable="testlabel" />
			</pack>
			<pack fill="x" expand="0" side="bottom">
				<label text="FOO" variable="foo" />
				<label text="BAR" variable="bar" />
				<label text="BINDTEST" variable="bindtest">
					<bind event="Button-1" callbackString="%W %T" virtual="0" />
				</label>
			</pack>
		</frame>
	</pack>
</xtk>
